{
  "release-type": "helm",
  "versioning": "default",
  "skip-github-release": true,
  "label": "autorelease",
  "release-label": "autorelease",
  "include-component-in-tag": true,
  "separate-pull-requests": true,
  "bump-minor-pre-major": true,
  "bump-patch-for-minor-pre-major": true,
  "plugins": [
    "sentence-case"
  ],
  "packages": {
    "charts/example": {
      "component": "example",
      "package-name": "example"
    }
  }
}
